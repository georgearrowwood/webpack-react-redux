{
  "name": "webpack-react-skeleton",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "all-run": "npm run all-build && npm run server-run",
    "all-build": "npm run server-build && npm run client-build",
    "server-run": "config=production node dist/server.bundle",
    "server-build": "BABEL_ENV=node babel-node webpack/buildServer.js",
    "client-build": "babel-node webpack/buildClient.js",
    "server-client-watch": "npm run client-watch & npm run server-watch",
    "client-watch": "babel-node webpack/hotLoadClient.js",
    "server-watch": "nodemon --watch server --watch app --exec npm run server-dev",
    "server-dev": "ENV=server NODE_ENV=development BABEL_ENV=node config=development PORT=8080 node_modules/babel-cli/bin/babel-node.js server",
    "test": "jest",
    "lint": "eslint server app"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@team-griffin/react-router-connected": "0.1.0",
    "axios": "0.16.2",
    "body-parser": "1.18.2",
    "cookie-parser": "1.4.3",
    "events": "1.1.1",
    "express": "4.16.2",
    "express-handlebars": "3.0.0",
    "express-joi-params": "0.1.2",
    "history": "4.7.2",
    "jsonwebtoken": "8.1.0",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-cookies": "0.1.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "3.7.2",
    "redux-form": "7.1.0",
    "redux-thunk": "2.2.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-require-ignore": "0.1.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-regenerator-runtime": "6.5.0",
    "css-loader": "0.28.7",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.2",
    "enzyme-to-json": "3.1.4",
    "eslint": "4.8.0",
    "eslint-config-airbnb": "16.0.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-node": "5.2.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-standard": "3.0.1",
    "extract-text-webpack-plugin": "3.0.1",
    "jest": "21.2.1",
    "node-sass": "4.5.3",
    "react-hot-loader": "3.0.0-beta.6",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "webpack": "3.6.0",
    "webpack-dev-server": "2.9.1"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./dev/jest-setup/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "**/app/**.{js,jsx}",
      "**/server/**.{js,jsx}"
    ]
  }
}
